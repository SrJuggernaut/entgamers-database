generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Clan {
  id               String   @id @default(uuid())
  name             String
  slug             String   @unique
  Description      String
  DiscordRoleID    String
  DiscordSectionID String
  DiscordData      Json
  Evento           Evento[]
}

model Evento {
  id          String @id @default(uuid())
  name        String
  slug        String @unique
  Description String
  image       String
  Content     Json
  clan        Clan?  @relation(fields: [clanId], references: [id])
  clanId      String
}
